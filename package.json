{
  "name": "base-project",
  "version": "1.0.0",
  "description": "Base project for Express + TypeScript architectures",
  "main": "dist/index.js",
  "scripts": {
    "build:docs": "typedoc",
    "start": "node -r module-alias/register dist/index.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "prettier:fix": "prettier --write \"src/**/*.ts\"",
    "lint-staged": "lint-staged",
    "typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/data/migrations/InitialMigration -d src/config/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/data-source.ts"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest",
      "^.+\\.js$": "ts-jest"
    },
    "moduleNameMapper": {
      "@config/(.*)": "<rootDir>/src/config/$1",
      "@models/(.*)": "<rootDir>/src/models/$1",
      "@dtos/(.*)": "<rootDir>/src/dtos/$1",
      "@controllers/(.*)": "<rootDir>/src/controllers/$1",
      "@routes/(.*)": "<rootDir>/src/routes/$1",
      "@middlewares/(.*)": "<rootDir>/src/middlewares/$1",
      "@services/(.*)": "<rootDir>/src/services/$1",
      "@utils/(.*)": "<rootDir>/src/utils/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ]
  },
  "_moduleAliases": {
    "@config": "dist/config",
    "@models": "dist/models",
    "@dtos": "dist/dtos",
    "@controllers": "dist/controllers",
    "@routes": "dist/routes",
    "@middlewares": "dist/middlewares",
    "@services": "dist/services",
    "@utils": "dist/utils"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/pino-http": "^5.8.4",
    "bcryptjs": "^3.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "pg": "^8.11.3",
    "pino": "^8.17.2",
    "pino-http": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.19"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.2.6",
    "pino-pretty": "^13.1.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.3.3"
  }
}
